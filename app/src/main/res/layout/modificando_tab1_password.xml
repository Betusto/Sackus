<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relaitveLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ModificandoActivity"
    android:focusable="true" android:focusableInTouchMode="true">

    <TextView
        android:id="@+id/activity_modificando_tab1_instruccionpassword3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="6dp"
        android:layout_marginTop="344dp"
        android:text="Confirme su nueva contraseña:"
        android:textSize="20sp" />

    <EditText
        android:id="@+id/activity_modificando_tab1_confirmarpasswordtext"
        android:layout_width="274dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/activity_modificando_tab1_passwordnuevotext"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="370dp"
        android:ems="10"
        android:hint="Confirmar contraseña"
        android:inputType="textPassword" />

    <ImageButton
        android:id="@+id/activity_modificando_tab1_visibility2"
        android:layout_width="wrap_content"
        android:layout_height="46dp"
        android:layout_alignTop="@+id/activity_modificando_tab1_passwordnuevotext"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:layout_toEndOf="@+id/activity_modificando_tab1_passwordnuevotext"
        android:background="@drawable/circleshape"
        android:src="@mipmap/ic_notvisible"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="288dp"
        tools:layout_editor_absoluteY="101dp" />

    <ImageButton
        android:id="@+id/activity_modificando_tab1_visibility3"
        android:layout_width="wrap_content"
        android:layout_height="46dp"
        android:layout_alignTop="@+id/activity_modificando_tab1_passwordnuevotext"
        android:layout_marginStart="0dp"
        android:layout_marginTop="117dp"
        android:layout_toEndOf="@+id/activity_modificando_tab1_passwordnuevotext"
        android:background="@drawable/circleshape"
        android:src="@mipmap/ic_notvisible"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="288dp"
        tools:layout_editor_absoluteY="101dp" />

    <EditText
        android:id="@+id/activity_modificando_tab1_passwordnuevotext"
        android:layout_width="274dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/activity_modificando_tab1_instruccionpassword1"
        android:layout_alignParentTop="true"
        android:layout_marginStart="7dp"
        android:layout_marginTop="253dp"
        android:ems="10"
        android:hint="Nueva contraseña"
        android:inputType="textPassword" />

    <TextView
        android:id="@+id/activity_modificando_tab1_instruccionpassword2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="6dp"
        android:layout_marginTop="226dp"
        android:text="Ingrese su nueva contraseña:"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/section_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginTop="@dimen/activity_vertical_margin"
        android:layout_marginEnd="@dimen/activity_horizontal_margin"
        android:layout_marginBottom="@dimen/activity_vertical_margin" />

    <TextView
        android:id="@+id/activity_modificando_tab1_instruccionpassword1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="6dp"
        android:layout_marginTop="113dp"
        android:text="Ingrese su contraseña actual:"
        android:textSize="20sp" />

    <Button
        android:id="@+id/activity_modificando_tab1_cambiarpasswordbutton"
        android:layout_width="155dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="445dp"
        android:text="Cambiar contraseña"
        android:background="@drawable/btn_uno"
        android:textColor="@drawable/txt_btn_uno"
        android:textAllCaps="true"
        android:textSize="14sp" />

    <EditText
        android:id="@+id/activity_modificando_tab1_passwordactualtext"
        android:layout_width="274dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/activity_modificando_tab1_instruccionpassword1"
        android:layout_alignParentTop="true"
        android:layout_marginStart="7dp"
        android:layout_marginTop="139dp"
        android:ems="10"
        android:hint="Contraseña actual"
        android:inputType="textPassword" />

    <ImageButton
        android:id="@+id/activity_modificando_tab1_visibility1"
        android:layout_width="wrap_content"
        android:layout_height="46dp"
        android:layout_alignTop="@+id/activity_modificando_tab1_passwordactualtext"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:layout_toEndOf="@+id/activity_modificando_tab1_passwordactualtext"
        android:background="@drawable/circleshape"
        android:src="@mipmap/ic_notvisible"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="288dp"
        tools:layout_editor_absoluteY="101dp" />


</RelativeLayout>